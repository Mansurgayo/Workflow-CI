# MLProject
name: diabetes_prediction_project
docker_env:
  image: python:3.8-slim

entry_points:
  main:
    parameters:
      data_path: {type: str, default: "/MLProject/dataset/cleaned_dataset.csv"}
      experiment_name: {type: str, default: "Diabetes_Prediction_CI"}
    command: "python modelling.py --data_path={data_path} --experiment_name={experiment_name}"